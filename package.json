        "eslint": "^8.0.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-jsdoc": "^40.0.2",
        "eslint-plugin-prefer-arrow": "^1.2.3",
        "husky": "^8.0.1",
        "lint-staged": "^13.0.0",
        "prettier": "2.8.7",
